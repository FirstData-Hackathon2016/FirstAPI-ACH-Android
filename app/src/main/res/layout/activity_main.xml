<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_below="@+id/tabcontent"
    android:layout_alignParentBottom="true"
    tools:context="com.firstdata.rashmi.rashmidemo.com.firstdata.ach.connectpay.MainActivity">


    <Button
        android:id="@+id/payRegulerBttn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"

        android:layout_weight="1"
        android:text="Reguler Enrollment" />

    <Button
        android:id="@+id/payPWMBBttn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_below="@+id/payRegulerBttn"
        android:layout_weight="1"
        android:text="PWMB Enrollment" />

</LinearLayout>